{
  "name": "@ckirby/keyv-sqlite",
  "version": "5.0.1",
  "description": "A new SQLite storage adapter for Keyv.",
  "keywords": [
    "keyv",
    "sqlite",
    "node"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": "Huijie Wei",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "bench": "vitest bench",
    "test": "vitest --ui",
    "check": "biome check --write ./src"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.2.0",
    "@keyv/test-suite": "^2.0.2",
    "@types/node": "^22.18.0",
    "@vitest/ui": "3.2.4",
    "keyv": "^5.2.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "keyv": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/keyv-sqlite.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "volta": {
    "node": "22.19.0"
  }
}
